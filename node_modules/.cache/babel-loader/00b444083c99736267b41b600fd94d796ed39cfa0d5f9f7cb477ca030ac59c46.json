        \n            <v-spacer></v-spacer>\n            \n            <v-select\n              v-model=\"filterCategory\"\n              :items=\"['全部', ...categories]\"\n              label=\"筛选类别\"\n              variant=\"outlined\"\n              density=\"compact\"\n              style=\"max-width: 200px;\"\n              @change=\"filterExchanges\"\n            ></v-select>\n            \n            <v-text-field\n              v-model=\"searchQuery\"\n              label=\"搜索物品\"\n              prepend-inner-icon=\"mdi-magnify\"\n              variant=\"outlined\"\n              density=\"compact\"\n              style=\"max-width: 200px; margin-left: 16px;\"\n              @input=\"filterExchanges\"\n            ></v-text-field>\n          </v-toolbar>\n          \n          <v-container>\n            <v-row>\n              <v-col \n                v-for=\"item in displayedExchanges\" \n                :key=\"item.id\" \n                cols=\"12\" \n                sm=\"6\" \n                md=\"4\" \n                lg=\"3\"\n              >\n                <v-card hover>\n                  <v-img\n                    v-if=\"item.image_urls\"\n                    :src=\"item.image_urls.split(',')[0]\"\n                    height=\"200px\"\n                    cover\n                  ></v-img>\n                  \n                  <v-card-title class=\"pb-1\">{{ item.title }}</v-card-title>\n                  \n                  <v-card-subtitle class=\"pb-0\">\n                    <v-chip \n                      :color=\"getCategoryColor(item.category)\" \n                      label \n                      size=\"small\"\n                    >\n                      {{ item.category }}\n                    </v-chip>\n                  </v-card-subtitle>\n                  \n                  <v-card-text class=\"pt-2\">\n                    <div class=\"text-subtitle-2 grey--text\">状态：{{ item.condition }}</div>\n                    <div class=\"text